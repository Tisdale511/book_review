<h1>Title: <%= link_to @review.book.title, @review.book %></h1>
<h3>Author: <%= @review.book.author %></h3>
<%= link_to (image_tag @review.book.image_url), @review.book %>
<p><%= link_to @review.user.username, @review.user %> said: <%= @review.content %></p>
<%= link_to "Edit Review", edit_review_path %>
<%= link_to "Delete Review", @review, method: :delete, data: { confirm: "Are you sure you want to delete this review?" } %>
